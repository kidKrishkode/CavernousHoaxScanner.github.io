<body>
    <section id="verifi-window">
        <div class="boll-design"></div>
        <div class="verifi-component flo">
            <div class="verifi-component-left">
                <div class="left-component upper"></div>
                <div class="left-component lower">
                    <div class="header flx">
                        <div class="logo"></div>
                        <div class="name">
