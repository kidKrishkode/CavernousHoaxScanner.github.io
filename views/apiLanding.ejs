<script>
    pageSet.push('header', 'apiLandingPage', 'moreProduct', 'faq-help', 'footer');
    appearSet.push(['.product-list-a', 120], ['.api-workflow', 580], ['#api-pycode', 980], ['#code-glow', 980], ['#code-greet', 980], ['.product-list-b', 1280], ['#moreProduct', 1480], ['#faq-help', 1680]); 
    currentPage.push('api');
    document.getElementById('header').innerHTML = document.getElementById('header').textContent;
    document.getElementById('footer').innerHTML = document.getElementById('footer').textContent;
    document.getElementById('moreProduct').innerHTML = document.getElementById('moreProduct').textContent;
    document.getElementById('faq-help').innerHTML = document.getElementById('faq-help').textContent;
    document.getElementById('customerFeed').innerHTML = document.getElementById('customerFeed').textContent;
    codeset('Image Converter', 'Python');

    function codeset(name, lang){
        setTimeout(()=>{
            appointCode = eval(compiler.appointCode);
            document.getElementById('apiInSnipPython').innerText = appointCode(name, lang, compiler);
            // updateLine = eval(compiler.updateLineNumbers);
            pyInterpreter = eval(compiler.pyInterpreter);
            ideDeploy = eval(compiler.ideDeploy);
            pyInterpreter('#apiInSnipPython');
            ideDeploy('#apiInSnipPython','#apiInSnipPython-line', 0);
        },1000);
    }
</script>
<script>
    setTimeout(()=>{
        const trafficAnalyser = eval(config.hex.trafficAnalyser);
        const line_graph = trafficAnalyser();

        const xValues = line_graph.xValues;
        const yValues = line_graph.yValues;

        new Chart("lineChart", {
            type: "line",
            data: {
                labels: line_graph.labels,
                datasets: [{
                    fill: true,
                    lineTension: 0,
                    backgroundColor: "rgba(110, 19, 175, 0.3)",
                    borderColor: "rgba(50, 0, 255, 0.1)",
                    data: yValues
                }]
            },
            options: {
                legend: { display: false },
                title: {
                    display: true,
                    text: "Visitor density according to Time flow"
                },
                scales: {
                    yAxes: [{ticks: {min: 0, max: 60}}],
                }
            }
        });
    },1000);

    setTimeout(()=>{
        let pie = document.querySelector('.pieChart');
        let ul = document.querySelector('.pie-ul');
        let popularityTest = eval(config.hex.popularityTest);
        popularityTest(ul, pie);
    },1000);
</script>
